#include <iostream>
#include <string>
using namespace std;

int main()
{
    system("chcp 1251");
    setlocale(LC_ALL, "Russian");
    string s, x, y;
    getline(cin, s);
    int j = 0;
    for (int i = 0; i <= s.size(); i++) {
        if (s[i] != ' ') x.append(1, s[i]);
        else {
            if (x != "") {
                if (j > 0) y.insert(0, 1, ' ');
                y.insert(0, x);
                x = "";
                j++;
            }
        }
    }
    y.insert(0, 1, ' ');
    y.insert(0, x);
    cout << y;
}
